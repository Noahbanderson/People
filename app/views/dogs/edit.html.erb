<h1>Edit Dog Form</h1>
<%= form_for @dog do |f| %>
    <%= f.label :animal_name %>
    <%= f.text_field :animal_name %>

    <%= f.label :age %>
    <%= f.text_field :age %>

    <%= f.label :breed %>
    <%= f.text_field :breed %>

    <%= f.label :person_id %>
    <%= f.text_field :person_id %>

    <%= f.submit %>

    <% end %>